{
    "manifest_version": 3,
    "name": "Picuki Switch",
    "version": "0.3",
    "description": "Redirects Instagram URLs to Picuki.com",
    "homepage_url": "https://github.com/claromes/picukiswitch",
    "author": "Claromes",
    "permissions": [
      "tabs"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "service_worker.js"
    }
}
